    ></v-text-field>\n                    </v-col>\n                    <v-col cols=\"12\">\n                      <v-text-field\n                        v-model=\"password\"\n                        :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\n                        :rules=\"[rules.required, rules.min]\"\n                        :type=\"show1 ? 'text' : 'password'\"\n                        name=\"input-10-1\"\n                        label=\"*Šifra\"\n                        hint=\"At least 8 characters\"\n                        counter\n                        @click:append=\"show1 = !show1\"\n                      ></v-text-field>\n                    </v-col>\n                    <v-col cols=\"12\">\n                      <v-text-field\n                        block\n                        v-model=\"verify\"\n                        :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\n                        :rules=\"[rules.required, passwordMatch]\"\n                        :type=\"show1 ? 'text' : 'password'\"\n                        name=\"input-10-1\"\n                        label=\"*Potvrdite šifru\"\n                        counter\n                        @click:append=\"show1 = !show1\"\n                      ></v-text-field>\n                    </v-col>\n                    <v-spacer></v-spacer>\n                    <v-col class=\"d-flex ml-auto\" cols=\"12\" sm=\"3\" xsm=\"12\">\n                      <v-btn\n                        x-large\n                        block\n                        :disabled=\"!validRegistration\"\n                        color=\"success\"\n                        @click=\"validateRegistration\"\n                        :loading=\"registrationLoading\"\n                        >Register</v-btn\n                      >\n                    </v-col>\n                  </v-row>\n                </v-form>\n              </v-card-text>\n            </v-card>\n\n    </div>\n  </div>\n    </v-dialog>\n</v-app>\n</template>\n\n<script>\nimport Swal from \"sweetalert2\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"UpdateUser\",\n  props: {\n    dialog: null,\n  },\n  data() {\n    return {\n      loading: false,\n      firstName: \"\",\n      lastName: \"\",\n      email: \"\",\n      password: \"\",\n      confirmPassword: \"\",\n    };\n  },\n  methods: {\n    updateUser() {\n      this.loading = true;\n      if (this.$refs.updateUserForm.validate()) {\n        const data = {\n          firstName: this.firstName,\n          lastName: this.lastName,\n          email: this.email,\n          password: this.password,\n        };\n        axios.post(\"/api/user/edit/1\", data).then((response) => {\n          this.loading = false;\n          console.log(response.data);\n          if(response.data.id){\n            Swal.fire({\n            icon: \"success\",\n            title: \"Uspješne izmjene!\",\n            showConfirmButton: true,\n            confirmButtonText: \"Potvrdi\",\n            timer: 1500,\n          });  \n          this.clearForm();\n          }\n          \n        });\n      } else {\n        this.loading = false;\n      }\n    },\n    clearForm() {\n      this.firstName = \"\";\n      this.lastName = \"\";\n      this.email = \"\";\n      this.password = \"\";\n      this.confirmPassword = \"\";\n    },\n  },\n};\n</script>"],"sourceRoot":"src/components"}]}