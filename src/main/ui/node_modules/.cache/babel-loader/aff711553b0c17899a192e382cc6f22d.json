,UAAA,iBAAA,EAAA,SAJA;AAKA,UAAA,KAAA,EAAA;AALA,SAAA;;AAOA,QAAA,MAAA,CAAA,KAAA,CAAA,OAAA,CAAA,KAAA;AACA,OAXA;AAaA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA,EAAA,KAAA,KAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,KAAA,OAAA,EA5BA,CA8BA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AA3EA;AA7BA,CAAA","sourcesContent":["<template>\n  <v-row justify=\"center\" data-app>\n    <v-dialog v-model=\"dialog\" persistent max-width=\"650px\">\n      <v-card>\n        <v-card-title>\n          <span class=\"headline\">Dodaj oglas</span>\n        </v-card-title>\n        <v-card-text>\n          <v-container>\n            <v-form ref=\"addForm\" v-model=\"validAdd\" lazy-validation>\n              <v-row>\n                <v-col cols=\"12\" sm=\"12\" md=\"6\">\n                  <v-text-field\n                    label=\"Naziv*\"\n                    v-model=\"name\"\n                    required\n                    :rules=\"rules.required\"\n                  ></v-text-field>\n                </v-col>\n                <v-col cols=\"12\" sm=\"12\" md=\"6\">\n                  <v-text-field\n                    v-model=\"price\"\n                    label=\"Cijena*\"\n                    hint=\"Cijena izražena u eurima\"\n                    append-icon=\"mdi-currency-eur\"\n                    :rules=\"rules.price\"\n                  ></v-text-field>\n                </v-col>\n                <v-col cols=\"12\" sm=\"12\" md=\"4\">\n                  <v-select\n                    :items=\"categories\"\n                    v-model=\"categoryId\"\n                    label=\"Kategorija*\"\n                    item-text=\"name\"\n                    item-value=\"id\"\n                    single-line\n                    solo\n                    :rules=\"rules.required\"\n                  ></v-select>\n                </v-col>\n                <v-col cols=\"12\" sm=\"12\" md=\"4\">\n                  <v-select\n                    :items=\"types\"\n                    v-model=\"typeId\"\n                    label=\"Tip oglasa*\"\n                    item-text=\"name\"\n                    item-value=\"id\"\n                    single-line\n                    solo\n                    :rules=\"rules.required\"\n                  ></v-select>\n                </v-col>\n                <v-col cols=\"12\" sm=\"12\" md=\"4\">\n                  <v-select\n                    :items=\"cities\"\n                    v-model=\"cityId\"\n                    label=\"Grad*\"\n                    item-text=\"name\"\n                    item-value=\"id\"\n                    single-line\n                    solo\n                    :rules=\"rules.required\"\n                  ></v-select>\n                </v-col>\n                <v-col cols=\"12\">\n                  <v-textarea\n                    solo\n                    v-model=\"description\"\n                    name=\"input-7-4\"\n                    label=\"Opis oglasa\"\n                    value=\"\"\n                    hint=\"Opis oglasa\"\n                    :rules=\"rules.required\"\n                  ></v-textarea>\n                </v-col>\n                <v-col>\n                  <v-container>\n                    <v-file-input\n                      accept=\"image/*\"\n                      label=\"Dodaj slike\"\n                      prepend-icon=\"mdi-image\"\n                      multiple\n                      chips\n                      color=\"pink\"\n                      v-model=\"files\"\n                      @change=\"addFiles\"\n                    ></v-file-input>\n                    <v-row>\n                      <v-col sm=\"4\" v-for=\"(file, f) in files\" :key=\"f\">\n                        {{ file.name }}\n                        <img\n                          :ref=\"'file'\"\n                          src=\"//placehold.it/400/99cc77\"\n                          class=\"img-fluid\"\n                          :title=\"'file' + f\"\n                        />\n                      </v-col>\n                    </v-row>\n                  </v-container>\n                </v-col>\n              </v-row>\n            </v-form>\n          </v-container>\n          <small>*označava obavezno polje</small>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"blue darken-1\" text @click=\"closeDialog\">\n            Zatvori\n          </v-btn>\n          <v-btn color=\"blue darken-1\" text @click=\"addAd\"> Sačuvaj </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </v-row>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Swal from \"sweetalert2\";\nexport default {\n  props: {\n    dialog: null,\n    categories: null,\n    cities: null,\n    types: null,\n  },\n  name: \"AddAd\",\n  data() {\n    return {\n      files: [],\n      readers: [],\n      images: [],\n      validAdd: \"\",\n      name: \"\",\n      description: \"\",\n      cityId: \"\",\n      typeId: \"\",\n      categoryId: \"\",\n      price: \"\",\n      rules: {\n        required: [(v) => !!v || \"Obavezno polje\"],\n        price: [\n          (v) => !!v || \"Obavezno polje\",\n          (v) => parseFloat(v) > 0 || \"Cijena mora biti pozitivan broj\",\n        ],\n      },\n    };\n  },\n  methods: {\n    addFiles() {\n      console.log(\"files\", this.files);\n      this.files.forEach((file, f) => {\n        this.readers[f] = new FileReader();\n        this.readers[f].onloadend = (e) => {\n          e;\n          let fileData = this.readers[f].result;\n          this.imgToBlob(this.readers[f]);\n          let imgRef = this.$refs.file[f];\n          imgRef.src = fileData;\n          console.log(fileData);\n          // send to server here...\n        };\n\n        this.readers[f].readAsDataURL(this.files[f]);\n      });\n    },\n    closeDialog() {\n      this.$emit(\"closeDialog\");\n      this.$refs.addForm.reset();\n    },\n    imgToBlob(img) {\n      blobUtil.arrayBufferToBlob(img.result, img.result.type).then(function (blob) {\n          console.log(\"here is a blob\", blob);\n          console.log(\"its size is\", blob.size);\n          console.log(\"its type is\", blob.type);\n          this.images.push({\n            adId: 1,\n            image: blob,\n          });\n        })\n        .catch(console.log.bind(console));\n      //fileReader.readAsArrayBuffer(file);\n    },\n    addAd() {\n      if (!this.$refs.addForm.validate()) {\n        return;\n      }\n      let data = {\n        name: this.name,\n        description: this.description,\n        categoryId: this.categoryId,\n        cityId: this.cityId,\n        ownerId: JSON.parse(localStorage.getItem(\"user\")).id,\n        statusId: 1,\n        typeId: this.typeId,\n        price: this.price,\n      };\n      axios.post(\"/api/ad\", data).then((response) => {\n        this.$emit(\"push\", response.data);\n        this.$emit(\"closeDialog\");\n        Swal.fire({\n          icon: \"success\",\n          title: \"Oglas uspješno objavljen!\",\n          showConfirmButton: true,\n          confirmButtonText: \"Potvrdi\",\n          timer: 1500,\n        });\n        this.$refs.addForm.reset();\n      });\n\n      console.log(\"files\", this.files);\n      console.log(\"readers\", this.readers);\n\n      // axios.post(\"/api/image\", dataForImages).then((response) => {\n      //   Swal.fire({\n      //     icon: \"success\",\n      //     title: \"Slike uspješno objavljene!\",\n      //     showConfirmButton: true,\n      //     confirmButtonText: \"Potvrdi\",\n      //     timer: 1500,\n      //   });\n      //   this.$refs.addForm.reset();\n      // });\n    },\n  },\n};\n</script>"],"sourceRoot":"src/components"}]}