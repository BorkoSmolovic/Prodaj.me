RA,KAAA;AAgBA,GA1BA;AA2BA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,EAAA,gBAAA,GAAA,EAAA;AACA,MAAA,GAAA;AACA,WAAA,IAAA,GAAA,KAAA,EAAA,CAAA,IAAA;AACA,WAAA,WAAA,GAAA,KAAA,EAAA,CAAA,WAAA;AACA,WAAA,MAAA,GAAA,KAAA,EAAA,CAAA,MAAA;AACA,WAAA,MAAA,GAAA,KAAA,EAAA,CAAA,MAAA;AACA,WAAA,UAAA,GAAA,KAAA,EAAA,CAAA,UAAA;AACA,WAAA,KAAA,GAAA,KAAA,EAAA,CAAA,KAAA;AACA;AATA,GA3BA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,WAAA,KAAA,CAAA,aAAA;AACA,KAHA;AAIA,IAAA,KAJA,mBAIA;AAAA;;AACA,UAAA,CAAA,KAAA,KAAA,CAAA,OAAA,CAAA,QAAA,EAAA,EAAA;AACA;AACA;;AACA,UAAA,IAAA,GAAA;AACA,QAAA,EAAA,EAAA,KAAA,EAAA,CAAA,EADA;AAEA,QAAA,IAAA,EAAA,KAAA,IAFA;AAGA,QAAA,WAAA,EAAA,KAAA,WAHA;AAIA,QAAA,UAAA,EAAA,KAAA,UAJA;AAKA,QAAA,MAAA,EAAA,KAAA,MALA;AAMA,QAAA,OAAA,EAAA,KAAA,EAAA,CAAA,OANA;AAOA,QAAA,QAAA,EAAA,KAAA,EAAA,CAAA,QAPA;AAQA,QAAA,MAAA,EAAA,KAAA,MARA;AASA,QAAA,KAAA,EAAA,KAAA;AATA,OAAA;AAWA,MAAA,KAAA,CAAA,GAAA,CAAA,kBAAA,KAAA,EAAA,CAAA,EAAA,EAAA,IAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,MAAA,EAAA,QAAA,CAAA,IAAA;;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,aAAA;;AACA,QAAA,IAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,KAAA,EAAA,4BAFA;AAGA,UAAA,iBAAA,EAAA,IAHA;AAIA,UAAA,iBAAA,EAAA,SAJA;AAKA,UAAA,KAAA,EAAA;AALA,SAAA;AAOA,OAVA;AAWA;AA9BA;AAtCA,CAAA","sourcesContent":["<template>\n  <v-row justify=\"center\" data-app>\n    <v-dialog v-model=\"dialog\" persistent max-width=\"650px\">\n      <v-card>\n        <v-card-title>\n          <span class=\"headline\">Izmijeni oglas</span>\n        </v-card-title>\n        <v-card-text>\n          <v-container>\n            <v-form ref=\"addForm\" v-model=\"validAdd\" lazy-validation>\n              <v-row>\n                <v-col cols=\"12\" sm=\"12\" md=\"6\">\n                  <v-text-field\n                    label=\"Naziv*\"\n                    v-model=\"name\"\n                    required\n                    :rules=\"rules.required\"\n                  ></v-text-field>\n                </v-col>\n                <v-col cols=\"12\" sm=\"12\" md=\"6\">\n                  <v-text-field\n                    v-model=\"price\"\n                    label=\"Cijena*\"\n                    hint=\"Cijena izražena u eurima\"\n                    append-icon=\"mdi-currency-eur\"\n                    :rules=\"rules.price\"\n                  ></v-text-field>\n                </v-col>\n                <v-col cols=\"12\" sm=\"12\" md=\"4\">\n                  <v-select\n                    :items=\"categories\"\n                    v-model=\"categoryId\"\n                    label=\"Kategorija*\"\n                    item-text=\"name\"\n                    item-value=\"id\"\n                    single-line\n                    solo\n                    :rules=\"rules.required\"\n                  ></v-select>\n                </v-col>\n                <v-col cols=\"12\" sm=\"12\" md=\"4\">\n                  <v-select\n                    :items=\"types\"\n                    v-model=\"typeId\"\n                    label=\"Tip oglasa*\"\n                    item-text=\"name\"\n                    item-value=\"id\"\n                    single-line\n                    solo\n                    :rules=\"rules.required\"\n                  ></v-select>\n                </v-col>\n                <v-col cols=\"12\" sm=\"12\" md=\"4\">\n                  <v-select\n                    :items=\"cities\"\n                    v-model=\"cityId\"\n                    label=\"Grad*\"\n                    item-text=\"name\"\n                    item-value=\"id\"\n                    single-line\n                    solo\n                    :rules=\"rules.required\"\n                  ></v-select>\n                </v-col>\n                <v-col cols=\"12\">\n                  <v-textarea\n                    solo\n                    v-model=\"description\"\n                    name=\"input-7-4\"\n                    label=\"Opis oglasa\"\n                    value=\"\"\n                    hint=\"Opis oglasa\"\n                    :rules=\"rules.required\"\n                  ></v-textarea>\n                </v-col>\n              </v-row>\n            </v-form>\n          </v-container>\n          <small>*označava obavezno polje</small>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"blue darken-1\" text @click=\"closeDialog\">\n            Zatvori\n          </v-btn>\n          <v-btn color=\"blue darken-1\" text @click=\"addAd\"> Sačuvaj </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </v-row>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Swal from \"sweetalert2\";\nexport default {\n  props: {\n    dialog: null,\n    categories: null,\n    cities: null,\n    types: null,\n    ad: null,\n  },\n  name: \"EditAd\",\n  data() {\n    return {\n      validAdd: \"\",\n      name: \"\",\n      description: \"\",\n      cityId: \"\",\n      typeId: \"\",\n      categoryId: \"\",\n      price: \"\",\n      rules: {\n        required: [(v) => !!v || \"Obavezno polje\"],\n        price: [\n          (v) => !!v || \"Obavezno polje\",\n          (v) => parseFloat(v) > 0 || \"Cijena mora biti pozitivan broj\",\n        ],\n      },\n    };\n  },\n  watch: {\n    dialog: function (val) {\n      val;\n      this.name = this.ad.name;\n      this.description = this.ad.description;\n      this.cityId = this.ad.cityId;\n      this.typeId = this.ad.typeId;\n      this.categoryId = this.ad.categoryId;\n      this.price = this.ad.price;\n    },\n  },\n  methods: {\n    closeDialog() {\n      this.$emit(\"closeDialog\");\n    },\n    addAd() {\n      if (!this.$refs.addForm.validate()) {\n        return;\n      }\n      let data = {\n        id: this.ad.id,\n        name: this.name,\n        description: this.description,\n        categoryId: this.categoryId,\n        cityId: this.cityId,\n        ownerId: this.ad.ownerId,\n        statusId: this.ad.statusId,\n        typeId: this.typeId,\n        price: this.price,\n      };\n      axios.put(\"/api/ad/edit/\" + this.ad.id, data).then((response) => {\n        this.$emit(\"edit\", response.data);\n        this.$emit(\"closeDialog\");\n        Swal.fire({\n          icon: \"success\",\n          title: \"Oglas uspješno izmijenjen!\",\n          showConfirmButton: true,\n          confirmButtonText: \"Potvrdi\",\n          timer: 1500,\n        });\n      });\n    },\n  },\n};\n</script>"],"sourceRoot":"src/components"}]}